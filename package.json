{
  "name": "fortnite-item-shop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "tsc && xo"
  },
  "dependencies": {
    "bent": "^7.3.12",
    "node-vibrant": "^3.2.1-alpha.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/bent": "^7.3.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint-config-xo-react": "^0.24.0",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "typescript": "^4.1.2",
    "vite": "^2.1.5",
    "xo": "^0.38.2"
  },
  "xo": {
    "prettier": true,
    "extends": "xo-react",
    "rules": {
      "unicorn/import-index": [
        "off"
      ],
      "unicorn/no-array-reduce": [
        "off"
      ],
      "import/extensions": [
        "off"
      ],
      "import/no-useless-path-segments": [
        "off"
      ],
      "unicorn/no-fn-reference-in-iterator": [
        "off"
      ],
      "unicorn/no-reduce": [
        "off"
      ],
      "import/no-named-as-default": [
        "off"
      ],
      "@typescript-eslint/prefer-literal-enum-member": [
        "off"
      ],
      "react/no-array-index-key": [
        "off"
      ],
      "unicorn/no-new-array": [
        "off"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": true,
    "useTabs": false,
    "endOfLine": "lf",
    "trailingComma": "es5"
  }
}
